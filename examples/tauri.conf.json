{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "ffnodes",
"version": "1.0.1",
  "identifier": "gh.drew-chase.ffnodes",
  "build": {
    "devUrl": "http://localhost:1420",

    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",

    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "FFNodes",
        "width": 800,
        "height": 600,
        "minHeight": 440,
        "minWidth": 740,
        "decorations": false
        
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/icon.png",
      "icons/icon.ico"
    ]
  }
}
